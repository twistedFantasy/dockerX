groups:
- name: nginx
  rules:
    - alert: nginx_down
      expr: probe_success{instance="nginx:80",job="nginx"} == 0
      for: 1m
      labels:
        severity: critical
      annotations:
        summary: "Nginx down (instance {{ $labels.instance }})"
        description: "Nginx down for 1m\n  VALUE = {{ $value }}\n  LABELS: {{ $labels }}"

